{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "expect": "readonly",
    "request": "readonly"
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "camelcase": "off",
    "no-unused-vars": "warn",
    "consistent-return": "warn"
  }
}
